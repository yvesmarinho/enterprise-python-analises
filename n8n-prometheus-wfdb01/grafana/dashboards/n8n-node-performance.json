{
  "uid": "n8n-node-analysis",
  "title": "N8N Node Performance Analysis",
  "tags": [
    "n8n",
    "performance",
    "nodes"
  ],
  "timezone": "browser",
  "schemaVersion": 16,
  "version": 0,
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "gridPos": {
        "x": 0,
        "y": 10,
        "w": 12,
        "h": 10
      },
      "type": "table",
      "title": "Top 20 Slowest Nodes (Average Time)",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "topk(20, n8n_node_execution_time_ms)",
          "format": "table",
          "instant": true,
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "left",
            "filterable": true
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 100,
                "color": "yellow"
              },
              {
                "value": 500,
                "color": "orange"
              },
              {
                "value": 1000,
                "color": "red"
              }
            ]
          }
        }
      },
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "displayName": "Avg Time (ms)",
            "desc": true
          }
        ]
      },
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "__name__": true
            },
            "renameByName": {
              "workflow_name": "Workflow",
              "node_name": "Node",
              "Value": "Avg Time (ms)"
            }
          }
        }
      ]
    },
    {
      "id": 2,
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 12,
        "h": 10
      },
      "type": "bargauge",
      "title": "Average Time by Node Type (milliseconds)",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "sort_desc(n8n_node_type_avg_time_ms)",
          "instant": true,
          "legendFormat": "{{node_type}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 50,
                "color": "yellow"
              },
              {
                "value": 200,
                "color": "orange"
              }
            ]
          },
          "custom": {
            "displayMode": "gradient",
            "scaleDistribution": {
              "type": "linear"
            }
          },
          "decimals": 0
        }
      },
      "options": {
        "orientation": "horizontal",
        "displayMode": "gradient",
        "showUnfilled": true,
        "minVizWidth": 0,
        "minVizHeight": 10
      }
    },
    {
      "id": 3,
      "gridPos": {
        "x": 12,
        "y": 10,
        "w": 12,
        "h": 10
      },
      "type": "table",
      "title": "All Nodes Performance",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "n8n_node_execution_time_ms",
          "format": "table",
          "instant": true,
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "filterable": true
          }
        }
      },
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "__name__": true
            },
            "indexByName": {
              "node_name": 0,
              "workflow_name": 1,
              "Value": 2
            },
            "renameByName": {
              "workflow_name": "Workflow",
              "node_name": "Node",
              "Value": "Avg Time (ms)"
            }
          }
        },
        {
          "id": "sortBy",
          "options": {
            "sort": [
              {
                "field": "Avg Time (ms)",
                "desc": true
              }
            ]
          }
        }
      ]
    },
    {
      "id": 4,
      "title": "HTTP Request por Workflow (Top 10 mais lentos)",
      "type": "bargauge",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 12,
        "h": 10
      },
      "targets": [
        {
          "expr": "topk(10, n8n_node_execution_time_ms{node_name=~\".*HTTP.*|.*Request.*|.*API.*\"})",
          "refId": "A",
          "legendFormat": "{{workflow_name}} \u2192 {{node_name}}"
        }
      ],
      "options": {
        "orientation": "horizontal",
        "displayMode": "gradient",
        "showUnfilled": true,
        "text": {
          "valueSize": 14
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 1000,
                "color": "yellow"
              },
              {
                "value": 3000,
                "color": "orange"
              },
              {
                "value": 5000,
                "color": "red"
              }
            ]
          }
        }
      },
      "transformations": [
        {
          "id": "sortBy",
          "options": {
            "sort": [
              {
                "field": "Value",
                "desc": true
              }
            ]
          }
        }
      ]
    }
  ]
}